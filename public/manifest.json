{
  "name": "Project Ascension",
  "manifest_version": 2,
  "version": "0.1",
  "browser_action": {},
  "description": "Testing voice control browser",
  "chrome_url_overrides": {
    "newtab": "./index.html"
  },
  "background": {
    "scripts": ["./background/background.js"],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "*://*/*",
    "identity",
    "activeTab",
    "identity.email"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
